{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "esModuleInterop": true,
    "rootDir": "."
  },
  "compileOnSave": true,
  "include": [
    ".", "tsconfig.json", "./.eslintrc.js",
  ],
  "exclude": [
    "node_modules",
  ]
}
